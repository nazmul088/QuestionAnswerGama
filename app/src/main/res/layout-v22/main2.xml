<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/root"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#f5a">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/src"
        app:cardElevation="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:cardCornerRadius="5dp"
        app:layout_constraintBottom_toTopOf="@+id/des1"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/lsrc"
            />
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/des1"
        app:layout_constraintTop_toBottomOf="@+id/src"
        app:layout_constraintBottom_toTopOf="@+id/des2"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ldes1"
            />
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/des2"
        app:layout_constraintTop_toBottomOf="@+id/des1"
        app:layout_constraintBottom_toTopOf="@+id/des3"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ldes2"
            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/des3"
        app:layout_constraintTop_toBottomOf="@+id/des2"
        app:layout_constraintBottom_toTopOf="@+id/des4"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ldes3"
            />
    </androidx.cardview.widget.CardView>
    
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/des4"
        app:layout_constraintTop_toBottomOf="@+id/des3"
        app:layout_constraintBottom_toTopOf="@+id/des5"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ldes4"
            />
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/des5"
        app:layout_constraintTop_toBottomOf="@+id/des4"
        app:layout_constraintBottom_toTopOf="@+id/des6"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ldes5"
            />
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/des6"
        app:layout_constraintTop_toBottomOf="@+id/des5"
        app:layout_constraintBottom_toTopOf="@+id/des7"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ldes6"
            />
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/des7"
        app:layout_constraintTop_toBottomOf="@+id/des6"
        app:layout_constraintBottom_toTopOf="@+id/des8"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ldes7"
            />
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/des8"
        app:layout_constraintTop_toBottomOf="@+id/des7"
        app:layout_constraintBottom_toTopOf="@+id/des9"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ldes8"
            />
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/des9"
        app:layout_constraintTop_toBottomOf="@+id/des8"
        app:layout_constraintBottom_toTopOf="@+id/des10"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ldes9"
            />
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/des10"
        app:layout_constraintTop_toBottomOf="@+id/des9"
        app:layout_constraintBottom_toBottomOf="parent"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ldes10"
            />
    </androidx.cardview.widget.CardView>




</androidx.constraintlayout.widget.ConstraintLayout>